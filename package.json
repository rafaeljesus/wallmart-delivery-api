{
  "name": "wallmart-delivery-api",
  "version": "1.0.0",
  "description": "Wallmart Delivery Rest API",
  "main": "app.js",
  "scripts": {
    "seed": "iojs bin/seed",
    "start": "iojs bin/easy.schedule",
    "test": "NODE_ENV=test npm run lint && npm run coverage",
    "lint": "eslint .",
    "watch:server": "nodemon -q -w api/ -x iojs --stage 0 -- bin/easy.schedule",
    "watch:test": "npm test -- -w",
    "posttest": "istanbul check-coverage --statements 90 --branches 70 --functions 90 --lines 90",
    "coverage": "istanbul cover --print both ./node_modules/mocha/bin/_mocha -- test --recursive",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=8984b0e812d39325d41bc6e409873ea4387660e75062cd694b60e4ba1b codeclimate-test-reporter < coverage/lcov.info"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "co-mocha": "^1.1.2",
    "codeclimate-test-reporter": "^0.1.1",
    "eslint": "^1.3.1",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.0",
    "nodemon": "^1.4.1",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-body": "^4.0.0",
    "co-monk": "^1.0.0",
    "debug": "^2.2.0",
    "kcors": "^1.0.1",
    "koa": "^1.0.0",
    "koa-body": "^1.2.1",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.1.2",
    "monk": "^1.0.1",
    "priorityqueuejs": "^1.0.0"
  },
  "engines": {
    "iojs": "3.3.0"
  },
  "keywords": [
    "API",
    "Rest",
    "Wallmart",
    "logistics"
  ],
  "author": "Rafael Jesus <rafaelljesus86@gmail.com>",
  "license": "MIT"
}
